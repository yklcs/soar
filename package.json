{
  "name": "soar",
  "version": "0.6.1",
  "description": "Soar, a minimal opinionated static site generator.",
  "main": "./lib/index.js",
  "bin": "./lib/cli.js",
  "keywords": [
    "static site generator",
    "ssg",
    "jsx"
  ],
  "author": "Lucas Yunkyu Lee <lucas@yklcs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yklcs/soar.git"
  },
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./jsx-runtime": {
      "default": "./lib/jsx.js",
      "types": "./lib/jsx.d.ts"
    }
  },
  "scripts": {
    "prepack": "pnpm run build",
    "build": "tsc",
    "format": "biome format --write **/*",
    "start": "node ./lib/cli.js"
  },
  "type": "module",
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "browserslist": "^4.23.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "esbuild": "^0.20.1",
    "express": "^4.18.2",
    "globby": "^14.0.1",
    "katex": "^0.16.9",
    "lightningcss": "^1.24.0",
    "linkedom": "^0.16.8",
    "ora": "^8.0.1",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/express": "^4.17.21",
    "@types/mdx": "^2.0.11",
    "@types/node": "^20.11.20",
    "@types/serve-static": "^1.15.5",
    "typescript": "^5.3.3",
    "unified": "^11.0.4"
  },
  "files": [
    "lib/",
    "src/",
    "LICENSE.txt",
    "package.json",
    "tsconfig.json",
    "README.md"
  ]
}
